{
  "version": 1,
  "projects": {
    "nx-apollo": {
      "root": "apps/nx-apollo",
      "sourceRoot": "apps/nx-apollo/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/nx-apollo",
            "index": "apps/nx-apollo/src/index.html",
            "main": "apps/nx-apollo/src/main.tsx",
            "polyfills": "apps/nx-apollo/src/polyfills.ts",
            "tsConfig": "apps/nx-apollo/tsconfig.app.json",
            "assets": [
              "apps/nx-apollo/src/favicon.ico",
              "apps/nx-apollo/src/assets"
            ],
            "styles": ["apps/nx-apollo/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/nx-apollo/src/environments/environment.ts",
                  "with": "apps/nx-apollo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "nx-apollo:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "nx-apollo:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/nx-apollo/.eslintrc",
            "tsConfig": [
              "apps/nx-apollo/tsconfig.app.json",
              "apps/nx-apollo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/nx-apollo/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nx-apollo/jest.config.js",
            "tsConfig": "apps/nx-apollo/tsconfig.spec.json"
          }
        }
      }
    },
    "nx-apollo-e2e": {
      "root": "apps/nx-apollo-e2e",
      "sourceRoot": "apps/nx-apollo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nx-apollo-e2e/cypress.json",
            "tsConfig": "apps/nx-apollo-e2e/tsconfig.e2e.json",
            "devServerTarget": "nx-apollo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nx-apollo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/nx-apollo-e2e/.eslintrc",
            "tsConfig": ["apps/nx-apollo-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/nx-apollo-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/api/.eslintrc",
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
    "feature-sets": {
      "root": "libs/feature-sets",
      "sourceRoot": "libs/feature-sets/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/feature-sets/.eslintrc",
            "tsConfig": [
              "libs/feature-sets/tsconfig.lib.json",
              "libs/feature-sets/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/feature-sets/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/feature-sets/jest.config.js",
            "tsConfig": "libs/feature-sets/tsconfig.spec.json"
          }
        }
      }
    },
    "data-access": {
      "root": "libs/data-access",
      "sourceRoot": "libs/data-access/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/data-access/.eslintrc",
            "tsConfig": [
              "libs/data-access/tsconfig.lib.json",
              "libs/data-access/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/data-access/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/data-access/jest.config.js",
            "tsConfig": "libs/data-access/tsconfig.spec.json"
          }
        },
        "generate": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx graphql-codegen --config libs/data-access/codegen.yml"
              }
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "nx-apollo"
}
